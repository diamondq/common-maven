    <scm>
        <connection>scm:git:git:github.com:diamondq/${scm.project}.git</connection>
        <developerConnection>scm:git:ssh://github.com:diamondq/${scm.project}.git</developerConnection>
        <url>https://github.com/diamondq/${scm.project}/tree/master</url>
    </scm>
    <profiles>
        <profile>
            <id>diamondq-generate-i18n-strings</id>
            <activation>
                <file>
                    <exists>src/i18n/resources</exists>
                </file>
            </activation>
            <properties>
                <i18n.implementation.class>com.diamondq.common.utils.misc.internationalization.MessagesEnum</i18n.implementation.class>
            </properties>
        </profile>
        <profile>
            <id>diamondq-osgi-project</id>
            <activation>
                <file>
                    <exists>profiles/osgi</exists>
                </file>
            </activation>
            <properties>
                <osgi.base.package>com.diamondq</osgi.base.package>
            </properties>
        </profile>
        <profile>
            <id>diamondq-deploy-self</id>
            <activation>
                <property>
                    <name>deploy</name>
                    <value>self</value>
                </property>
            </activation>
            <properties>
                <scm.project>common-maven</scm.project>
            </properties>
        </profile>
        <profile>
            <id>diamondq-sign</id>
            <activation>
                <property>
                    <name>jenkins</name>
                    <value>true</value>
                </property>
            </activation>
            <properties>
                <gpg.arg1>--pinentry-mode</gpg.arg1>
                <gpg.arg2>loopback</gpg.arg2>
            </properties>
        </profile>
    </profiles>
    <repositories>
        <repository>
            <id>dq-snapshot</id>
            <url>https://nexus.diamondq.com/repository/maven-snapshots/</url>
            <snapshots>
