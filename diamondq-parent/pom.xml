                <updatePolicy>${snapshot.update.policy}</updatePolicy>
            </snapshots>
            <releases>
                <enabled>false</enabled>
            </releases>
        </repository>
        <repository>
            <id>dq-release</id>
            <url>https://nexus.diamondq.com/repository/maven-public/</url>
            <snapshots>
                <enabled>false</enabled>
